   -
     name : ProxyEtat
     internal_properties : CAAAAAgAAAA4sI0y1HwXOtRl0TWGrJW0lY7rbx5uPWx4HXRLhLRtUWkwG/Z/CmwGvQJLJ+rXGYcyZY/ihYFJfFvuSgSUBh6kF2N4NZQ9WaUE4/VJJ5/HLwB7DLCcl1hnJ0TfZyzVAWTuxW7uQ1ZGXa0giyT1Nxz9OA7UzXNCG+4HPDZGPSLIVRgwqvcWEkXLnnzhHXFToxotfZIHsNgcPvNnbrPGtSqGr8PqZr7b
     procedure_id : 1725512126310949305
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //ProxyEtat ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Procédure automatique :
      // La procédure est exécutée automatiquement, après le code d'initialisation de la fenêtre
      // Elle sera répétée en boucle, en attendant 1 seconde entre chaque appel
      // Chaque appel suivant exécute une seule fois la procédure, sans timer
      // 
      
      Procédure ProxyEtat()
      
      	// Gestion des options de menu
      	si gclProxy:EnExécution ALORS		
      
      		// Modifie l'icône
      		SysIconeModifie(fRepExe + ["\"] + "proxy-on.ico", "WD Proxy HTTP [Actif]")
      		
      		si OPT_Démarrer..Etat <> grise alors
      		
      			// Affiche un message en exécution
      			SysIconeAfficheBulle("Pour arrêter ou modifier les options du proxy, cliquez avec le bouton droit de la souris sur l'icône ci-dessous.", "Le proxy est en cours d'exécution", sysTypeBulleInfo)
      		FIN
      		
      		OPT_Démarrer..etat 	= grise
      		OPT_Arrêter..Etat 	= actif		
      
      	sinon
      			
      		// Modifie l'icône
      		SysIconeModifie(fRepExe + ["\"] + "proxy-off.ico", "WD Proxy HTTP [Inactif]")
      		
      		SI OPT_Démarrer..Etat <> actif ALORS
      			
      			// Affiche un message en exécution
      			SysIconeAfficheBulle("Pour démarrer ou modifier les options du proxy, cliquez avec le bouton droit de la souris sur l'icône ci-dessous.", "Le proxy est arrêté", sysTypeBulleAvertissement)
      		FIN
      		
      		OPT_Démarrer..Etat 	= actif
      		OPT_Arrêter..Etat 	= grise
      	FIN
      	
      	si fenetat(FEN_Configuration) <> inexistant alors
      		// On peut paramétrer le port si le proxy n'est pas actif
      		FEN_Configuration.SAI_PORT..etat = (gclProxy:EnExécution ? grise sinon actif)
      	FIN
     type : 458752
