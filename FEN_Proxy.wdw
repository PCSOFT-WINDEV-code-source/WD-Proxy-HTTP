     code : |1+
      // Résumé : Gère les événements de l'icone
      // Syntaxe :
      //[ <Résultat> = ] OnIconeSystray (<nIDBalloonTip> est entier, <nEvénement> est entier)
      //
      // Paramètres :
      //	nEvénement (entier) : Numéro de l'événement
      // Valeur de retour :
      // 	booléen : Faux
      //
      Procédure OnIconeSystray(local nEvénement est un entier)
      
      	// Double clic
      	si nEvénement = 515 ALORS
      		
